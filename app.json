{
  "expo": {
    "name": "ZeroFall",
    "slug": "zerofall",
    "version": "1.0.0",
    "scheme": "zerofall",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "image": "./assets/app_logo.png"
    },
    "icon": "./assets/app_logo.png",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.zerofall",
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.zerofall",
      "versionCode": 5,
      "permissions": [
        "ACCESS_WIFI_STATE",
        "ACCESS_NETWORK_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "icon": "./assets/app_logo.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "앱이 WiFi 네트워크를 확인하기 위해 위치 권한이 필요합니다."
        }
      ],
      "expo-font",
      "expo-localization"
    ],
    "notification": {
      "icon": "./assets/app_logo.png",
      "color": "#ffffff"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d0386660-2228-4773-a478-d72799d1f08d"
      }
    },
    "owner": "kershaw"
  }
}
